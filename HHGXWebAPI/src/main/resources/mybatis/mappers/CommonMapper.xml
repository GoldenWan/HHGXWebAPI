<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hhgx.soft.mappers.CommonMapper">
	<select id="getDictionaryByTypeName" resultType="dictionary">
	SELECT
		ItemName
	FROM
		dictionary
	WHERE
		TypeName = #{typeName}

	</select>
	
	</mapper>